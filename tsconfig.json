{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "jsx": "react",
    "module": "esnext",
    "moduleResolution": "node",
    "noImplicitAny": false,
    "target": "es5",
    "strictNullChecks": true,
    "alwaysStrict": true,
    "strict": true,
    "lib": ["dom", "es2015", "es2016", "es2017", "es2018"],
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "removeComments": true,
    "outDir": "dist",
  }
}
