{
  "compilerOptions": {
    "target": "es5",
    "module": "esnext",
    "lib": ["dom", "es2015", "es2016", "es2017", "es2018"],
    "outDir": "dist",
    "rootDir": "src",
    "jsx": "react",
    "allowSyntheticDefaultImports": true,
    "removeComments": true,
    "strict": true,
    "noImplicitAny": false,
    "esModuleInterop": true
  }
}
